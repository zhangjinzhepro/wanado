{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "prefer-rest-params": "off",
        "import/prefer-default-export": "off",
        "no-undef": "off",
        "no-plusplus": "off",
        "linebreak-style": ["error", "unix"]
    }
}
